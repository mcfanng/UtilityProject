{

  "Serilog": {
    "Using": [ "Serilog.Sinks.Console" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [ "Console", "File", "Debug" ],
    "Enrich": [ "FromLogContext" ],
    "Properties": {
      "Application": "UtilityApp"
    }
  }
}